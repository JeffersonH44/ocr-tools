  "homepage": "https://github.com/cheminfo-js/ocr#readme",
  "devDependencies": {
    "cheminfo-tools": "^1.5.0",
    "eslint": "^3.4.0",
    "eslint-config-cheminfo": "^1.2.0",
    "eslint-plugin-no-only-tests": "^1.1.0",
    "istanbul": "^0.4.4",
    "mocha": "^3.1.2",
    "mocha-better-spec-reporter": "^3.0.2",
